{
  "version": "1.0",
  "name": "r8s-job-updater",
  "func_name": "handler.lambda_handler",
  "resource_type": "lambda",
  "iam_role_name": "job-updater-role",
  "runtime": "python3.8",
  "memory": 128,
  "timeout": 100,
  "lambda_path": "/lambdas/r8s_job_updater",
  "dependencies": [
    {
      "resource_name": "r8s-job-state-update",
      "resource_type": "cloudwatch_rule_trigger"
    }
  ],
  "event_sources": [
    {
      "target_rule": "r8s-job-state-update",
      "resource_type": "cloudwatch_rule_trigger"
    }
  ],
  "env_variables": {
      "log_level": "${log_level}"
  },
  "publish_version": true,
  "alias": "${lambdas_alias_name}",
  "subnet_ids": [
    "${lambda_subnet_id}"
  ],
  "security_group_ids": [
    "${lambda_security_group}"
  ],
  "ephemeral_storage": 512,
  "url_config": {}
}